{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "student": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::student.student"
    },
    "reference_num": {
      "type": "string"
    },
    "unique_learners_id": {
      "type": "string"
    },
    "first_name": {
      "type": "string"
    },
    "middle_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "suffix": {
      "type": "string"
    },
    "address": {
      "type": "text"
    },
    "mother_name": {
      "type": "string"
    },
    "father_name": {
      "type": "string"
    },
    "contact_number": {
      "type": "string"
    },
    "birthplace": {
      "type": "text"
    },
    "sex": {
      "type": "string"
    },
    "civil_status": {
      "type": "string"
    },
    "employment": {
      "type": "string"
    },
    "educational_attainment": {
      "type": "string"
    },
    "age": {
      "type": "string"
    },
    "birthdate": {
      "type": "date"
    }
  }
}
